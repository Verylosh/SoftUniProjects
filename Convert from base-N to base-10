using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _1Exer
{
    class Program
    {
        static void Main(string[] args)
        {
            long[] input = Console.ReadLine().Split().Select(long.Parse).ToArray();
            long fromBase = input[0];
            long value = input[1];

            long result = FromBaseNToBaseTen(value, fromBase);

            Console.WriteLine(result);

        }
        public static long FromBaseNToBaseTen(long value, long fromBase)
        {
            string number = value.ToString();
            long n = 1;
            long result = 0;

            for (int i = number.Length - 1; i >= 0; --i)
            {
                result += n * (number[i] - '0');
                n *= fromBase;
            }

            return result;
        }
    }
}
